
<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <meta name="description" content="FZBS">
    <meta name="author" content="David">
    <meta name="keyword" content="Bootstrap,Admin,Template,Open,Source,AngularJS,Angular,Angular2,Angular 2,Angular4,Angular 4,jQuery,CSS,HTML,RWD,Dashboard,React,React.js,Vue,Vue.js">
    <title>@ViewData["Title"] - SMLYS</title>
    <!-- Icons -->
    <link href="~/vendors/css/font-awesome.min.css" rel="stylesheet">
    <link href="~/vendors/css/simple-line-icons.min.css" rel="stylesheet">
    <link href="~/css/temp/jquery-ui.css" rel="stylesheet" />
    <link href="~/css/temp/tutorial.css" rel="stylesheet" />
    <link href="~/css/temp/jquery.autocompleteInline.css" rel="stylesheet" />

    <!-- Main styles for this application -->
    <environment include="Development">
        <link rel="stylesheet" href="~/css/style.css">
        <link rel="stylesheet" href="~/css/site.css" />
    </environment>
    <environment exclude="Development">
        <link rel="stylesheet" href="~/css/style.css" asp-append-version="true" />
        <link rel="stylesheet" href="~/css/site.css" asp-append-version="true" />
    </environment>
    <!-- Styles required by this view -->
    @RenderSection("Styles", required: false)
</head>
<body class="app header-fixed sidebar-fixed aside-menu-fixed aside-menu-hidden">
    <!-- *APP-HEADER* -->
    <partial name="_AppHeader" />
    <!-- /*APP-HEADER* -->
    <div class="app-body">
        <div class="sidebar">
            <!-- *SIDEBAR-NAV* -->
            <partial name="_SidebarNav" />
            <!-- /*SIDEBAR-NAV* -->
            <button class="sidebar-minimizer brand-minimizer" type="button"></button>
        </div>
        <!-- *MAIN CONTENT* -->
        <main class="main">
            <!-- *BREADCRUMB* -->
            <partial name="_BreadCrumb" />
            <!-- /*BREADCRUMB* -->
            <!-- *CONTAINER-FLUID* -->
            <div class="container-fluid">
                <!-- *PAGE* -->
                @RenderBody()
                <!-- /*PAGE* -->
            </div>
            <!-- /*CONTAINER-FLUID* -->
        </main>
        <!-- /*MAIN CONTENT* -->
        <!-- *ASIDE-MENU* -->
        <partial name="_AsideMenu" />
        <!-- /*ASIDE-MENU* -->
    </div>
    <!-- /.modal -->
    <div class="modal fade" id="primaryModal" tabindex="-1" role="dialog" aria-labelledby="myModalLabel" aria-hidden="true">
        <div class="modal-dialog modal-primary" role="document">
            <div class="modal-content">
                <div class="modal-header">
                    <h4 class="modal-title">Modal title</h4>
                    <button type="button" class="close  header-close" data-dismiss="modal" aria-label="Close">
                        <span aria-hidden="true">×</span>
                    </button>
                </div>
                <div class="modal-body">
                    <p>One fine body…</p>
                </div>
                <div class="modal-footer">
                </div>
            </div>
            <!-- /.modal-content -->
        </div>
        <!-- /.modal-dialog -->
    </div>

    <footer class="app-footer">
        <span><a href="http://www.smlys.com"></a> © 2019 SMLYS.</span>
    </footer>
    <!-- Bootstrap and necessary plugins -->
    @*<script src="~/vendors/js/jquery.min.js"></script>*@
    <script src="https://code.jquery.com/jquery-1.12.4.min.js" integrity="sha256-ZosEbRLbNQzLpnKIkEdrPv7lOy9C27hHQ+Xp8a4MxAQ=" crossorigin="anonymous">></script>
    <script src="~/vendors/js/popper.min.js"></script>
    <script src="~/lib/temp/jquery-ui.js"></script>
    <script src="~/lib/temp/jquery.autocompleteInline.js"></script>
    <script src="~/vendors/js/bootstrap.min.js"></script>
    <script src="~/vendors/js/pace.min.js"></script>
    <!-- Plugins and scripts required by all views -->
    <script src="~/vendors/js/Chart.min.js"></script>
    <!-- CoreUI main scripts -->
    <script src="~/js/app.js"></script>
    <!-- Plugins and scripts required by this views -->
    <!-- Custom scripts required by this view -->
    <script src="~/js/common/SMLYS.js"></script>
    @*<script src="~/js/views/Invoices/invoiceDetail.js"></script>*@
    <script src="~/js/indexTest.js" ></script>
    @RenderSection("Scripts", required: false)
</body>
</html>